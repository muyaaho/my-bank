# Production Secrets Example
# Copy this file to secrets.env and update with real values
# Never commit secrets.env to version control!

JWT_SECRET=change-this-to-production-secret-minimum-256-bits-required-for-hs512
JWT_EXPIRATION=86400000

POSTGRES_USERNAME=mybank_prod
POSTGRES_PASSWORD=change-this-strong-password

MONGODB_USERNAME=mybank_prod
MONGODB_PASSWORD=change-this-strong-password
MONGODB_AUTH_DATABASE=admin

REDIS_PASSWORD=change-this-if-redis-auth-enabled

# OAuth Credentials
KAKAO_CLIENT_ID=your-kakao-client-id
KAKAO_CLIENT_SECRET=your-kakao-client-secret
KAKAO_REDIRECT_URI=https://app.mybank.com/auth/kakao/callback
